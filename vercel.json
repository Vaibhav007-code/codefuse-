{
  "version": 2,
  "builds": [
    {
      "src": "server/proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { 
        "distDir": "build",
        "buildCommand": "npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/proxy.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}
