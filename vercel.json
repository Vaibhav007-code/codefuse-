{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/proxy.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }